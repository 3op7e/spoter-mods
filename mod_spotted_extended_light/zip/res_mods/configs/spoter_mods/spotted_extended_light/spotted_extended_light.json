{
    "config": {
        "debug": false,           #activate debug message in log
        "enable": true,           #activate mod
        "icon_size": [70,24],     #size icons in text messages [width,height]
        "language": "Русский",    #select avaliable language in "language" section,
        "sound": true             #
    },
#sound select in actions, if need disable variable sound: use ""
# "0": spotted, "1": radio hit assist, "2": radio kill assist, "3": tracks assist
    "sound": {
        "0": "/GUI/notifications_FX/enemy_sighted_for_team",
        "1": "/GUI/notifications_FX/gun_intuition",
        "2": "/GUI/notifications_FX/cybersport_auto_search",
        "3": "/fortified_area/gui/gun_intuition"
    },
# You may add custom\new language in this section
# "0": spotted, "1": radio hit assist, "2": radio kill assist, "3": tracks assist
# avaliable macroses: 
#{icons} - list icons for target vehicles,
#{names} - list targets player names,
#{vehicles} - list targets vehicle names,
#{icons_names} - list targets icons and player names,
#{icons_vehicles} - list targets icons and vehicle names,
#{full} - list all data about targets
    "language": {
        "Deutsch": {
            "messages": {
                "0": "Gefunden {icons}",
                "1": "Schaden von Licht in {icons_names}",
                "2": "Bei gluht getotet {full}",
                "3": "Assist {icons_vehicles} fur schwer verletzte tracks"
            }
        },
        "English": {
            "messages": {
                "0": "Spotted {icons}",
                "1": "Radio hit assist to {icons_names}",
                "2": "Radio kill assist to {full}",
                "3": "Tracks assist {icons_vehicles}"
            }
        },
        "Русский": {
            "messages": {
                "0": "Засвечены {icons}",
                "1": "Урон по засвету в {icons_names}",
                "2": "Убили по засвету {full}",
                "3": "Ассист {icons_vehicles} за сбитые траки"
            }
        }
    }
}